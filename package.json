{
  "name": "FrontendProjectTemplateByKyoyababa",
  "version": "2.0.0",
  "description": "Frontend Project Template / kyoyababa.",
  "scripts": {
    "test": "karma start",
    "clean": "rm -rf ./**/.DS_Store npm-debug.log dist",
    "clean-install": "rm -rf ./node_modules && npm install",
    "transport": "(cpx \"public/assets/images/**/*\" \"dist/assets/images\")",
    "build:prod": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "npm run clean && npm run build:dev && npm run transport && webpack-dev-server --inline"
  },
  "author": "kyoyababa",
  "dependencies": {
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.0",
    "autoprefixer": "^9.4.2",
    "cpx": "^1.5.0",
    "css-loader": "^2.0.0",
    "ejs-compiled-loader": "^1.1.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.3.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript-preprocessor": "^0.4.0",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
