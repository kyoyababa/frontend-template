{
  "name": "frontend-template",
  "version": "2.0.2",
  "description": "Frontend Project Template.",
  "scripts": {
    "test": "karma start",
    "clean": "rm -rf ./**/.DS_Store npm-debug.log dist",
    "clean-install": "rm -rf ./node_modules && npm install",
    "transport": "(cpx \"public/assets/images/**/*\" \"dist/assets/images\")",
    "build:prod": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "npm run clean && npm run build:dev && npm run transport && webpack-dev-server --inline"
  },
  "author": "kyoyababa",
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "autoprefixer": "^9.7.5",
    "cpx": "^1.5.0",
    "css-loader": "^1.0.0",
    "ejs-compiled-loader": "^1.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "json-loader": "^0.5.7",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-url": "^7.3.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.5.0",
    "typescript": "^2.9.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
