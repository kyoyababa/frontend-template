{
  "name": "FrontendProjectTemplateByKyoyababa",
  "version": "2.0.0",
  "description": "Frontend Project Template / kyoyababa.",
  "scripts": {
    "test": "karma start",
    "clean": "rm -rf ./**/.DS_Store npm-debug.log dist",
    "clean-install": "rm -rf ./node_modules && npm install",
    "transport": "(cpx \"public/assets/images/**/*\" \"dist/assets/images\")",
    "build:prod": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "npm run clean && npm run build:dev && npm run transport && webpack-dev-server --inline"
  },
  "author": "kyoyababa",
  "dependencies": {
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "autoprefixer": "^9.7.5",
    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "ejs-compiled-loader": "^1.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "jasmine": "^3.5.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-typescript-preprocessor": "^0.4.0",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
